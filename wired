from BirdBrain import Finch
from time import sleep

bird = Finch()

bird.setMove('F',10,50)
bird.setMove('B',5,50)
bird.setTurn('R',90,50)
bird.setMove('F',3,50)
bird.setTurn('L',90,50)
bird.setMove('F',5,50)
bird.setMove('B',10,50)
bird.setTurn('R',90,50)
bird.setBeak(50,0,50)
sleep(10)
bird.setBeak(0,0,0)

bird.setMove('F',3,50)
bird.setBeak(50,50,0)
sleep(10)
bird.setBeak(0,0,0)

bird.setMove('F',3,50)
bird.setMove('B',3,50)
bird.setTurn('L',90,50)
bird.setMove('F',5,50)
bird.setTurn('R',90,50)
bird.setMove('F',3,50)
bird.setMove('B',3,50)
bird.setTurn('L',90,50)
bird.setMove('F',5,50)
bird.setTurn('R',90,50)
bird.setMove('F',3,50)
bird.setBeak(50,0,50)
sleep(10)
bird.setBeak(0,0,0)

bird.setMove('F',3,50)
bird.setBeak(50,50,0)
sleep(10)
bird.setBeak(0,0,0)

bird.setTurn('R',90,50)
bird.setMove('F',10,50)
bird.setTurn('L',90,50)
bird.setMove('F',3,50)
bird.setBeak(50,0,50)
sleep(10)
bird.setBeak(0,0,0)

bird.setMove('F',3,50)
