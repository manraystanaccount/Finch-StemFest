from BirdBrain import Finch
from time import sleep

myFinch = Finch(A)

myFinch.setMove('F',10,50)
myFinch.setMove('B',5,50)
myFinch.setTurn('R',90,50)
myFinch.setMove('F',3,50)
myFinch.setTurn('L',90,50)
myFinch.setMove('F',5,50)
myFinch.setMove('B',10,50)
myFinch.setTurn('R',90,50)
myFinch.setBeak(50,0,50)
sleep(10)
myFinch.setBeak(0,0,0)

myFinch.setMove('F',3,50)
myFinch.setBeak(50,50,0)
